{
  "root": "apps/ocr",
  "sourceRoot": "apps/ocr/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flask --app main run",
        "cwd": "apps/ocr/src"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flask --app main run",
        "cwd": "apps/ocr/src"
      }
    }
  }
}
