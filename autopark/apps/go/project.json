{
    "root": "apps/go",
    "sourceRoot": "apps/go/src",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go build -o='../../../dist/apps/go/' .",
          "cwd": "apps/go/src"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go run .",
          "cwd": "apps/go/src"
        }
      }
    }
  }